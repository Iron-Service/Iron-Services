function dirReduc(arr){
  for(let i = 1; i = arr.length; arr+=2){
    if(arr[i] === "WEST"){
      if(arr[i-1]=== "EAST"){
        arr.splice(i-1,2 )
      }
    }
    if(arr[i] === "EAST"){
      if(arr[i-1]=== "WEST"){
        arr.splice(i-1,2 )
      }
    }
    if(arr[i] === "NORTH"){
      if(arr[i-1]=== "SOUTH"){
        arr.splice(i-1,2 )
      }
    }
    if(arr[i] === "SOUTH"){
      if(arr[i-1]=== "NORTH"){
        arr.splice(i-1,2 )
      }
    }
  }
}
